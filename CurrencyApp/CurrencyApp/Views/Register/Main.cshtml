@model CurrencyApp.Models.RegisterModel



<div class="form-group row">
    <label asp-for="CurrencyCode" class="col-sm-2 col-form-label">Code</label>
    <div class="col-sm-10">
        <input asp-for="CurrencyCode" class="form-control" placeholder="Code">
        <span asp-validation-for="CurrencyCode" class="text-danger"></span>
    </div>
</div>
<div class="form-group row">
    <label asp-for="CurrencyName" class="col-sm-2 col-form-label">Name</label>
    <div class="col-sm-10">
        <input asp-for="CurrencyName" class="form-control" placeholder="">
        <span asp-validation-for="CurrencyName" class="text-danger"></span>
    </div>
</div>

<div class="form-group row">
    <label asp-for="CurrencyNameLatin" class="col-sm-2 col-form-label">Latin</label>
    <div class="col-sm-10">
        <input asp-for="CurrencyNameLatin" class="form-control" placeholder="">
        <span asp-validation-for="CurrencyNameLatin" class="text-danger"></span>
    </div>
</div>


<div class="form-group row">
    <label asp-for="OrderNum" class="col-sm-2 col-form-label">Order By</label>
    <div class="col-sm-10">
        <input asp-for="OrderNum" class="form-control" placeholder="">
        <span asp-validation-for="OrderNum" class="text-danger"></span>
    </div>
</div>




<div class="form-group row">
    <div class="col-sm-10">
        <button type="submit" class="btn btn-primary">add</button>
    </div>
</div>
